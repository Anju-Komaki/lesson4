<div class="col-xs-9">
	<h2>Users</h2>
		<table class="table">
			<thead>
				<tr>
					<th>image</th>
					<th>name</th>
					<th colspan="3"></th>
				</tr>
			</thead>
			<tbody>
				<tr>
				<% @users.each do |user| %>
				<td><%= attachment_image_tag @user, :profile_image, :fill, 30, 30, format: 'jpeg', class: "img-circle pull-left profile-img" %></td>
				<td><%= user.name %></td>
			 	<td><%= link_to "show", user_path(user.id) %>
				<% end %></td>
				</tr>
			</tbody>
			</table>
			</div>