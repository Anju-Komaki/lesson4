	<div id="likes_buttons_<%= @travel_image.id %>">
	<% if @travel_image.favorited_by?(current_user) %>
	<%= link_to travel_image_favorites_path(@travel_image), method: :delete, remote: true do %>
	<i class="fas fa-thumbs-up"></i><%= @travel_image.favorites.count %>
	<% end %>
	<% else %>
	<%= link_to travel_image_favorites_path(@travel_image), method: :post, remote: true do %>
	<i class="far fa-thumbs-up"></i><%= @travel_image.favorites.count %>
	<% end %>
	<% end %>
	</div>